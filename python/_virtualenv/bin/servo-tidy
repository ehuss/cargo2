#!/Users/eric/Proj/rust/servo/python/_virtualenv/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'servo-tidy','console_scripts','servo-tidy'
__requires__ = 'servo-tidy'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('servo-tidy', 'console_scripts', 'servo-tidy')()
    )
